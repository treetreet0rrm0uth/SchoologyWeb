{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "api/server.js"
        },
        {
            "src": "/execute/:district/:request",
            "dest": "api/server.js/execute/:district/:request"
        }
    ]
}
{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/view",
            "dest": "/index.js",
            "methods": ["GET"]
        },
        {
            "src": "/view2",
            "dest": "/index.js",
            "methods": ["GET"]
        },
        {
            "src": "/execute/(.+)/(.+)",
            "dest": "/index.js",
            "methods": ["GET"]
        }
    ]
}